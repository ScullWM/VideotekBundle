{% extends "SwmVideotekBundle:Admin:layout.html.twig" %}

{% block content %}
<div class="row">
    <div class="col-md-6">
        <form action="{{ path('video_admin_edit', {id:video.id})}}" class="form-horizontal" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}

            <input type="submit" class="btn btn-success" />
        </form>
    </div>
    <div class="col-md-6">
        {{videoExtended.videoInfo|raw}}
    </div>
</div>
{% endblock %}